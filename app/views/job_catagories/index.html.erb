<p id="notice"><%= notice %></p>

<h1>Job Catagories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_catagories.each do |job_catagory| %>
      <tr>
        <td><%= job_catagory.name %></td>
        <td><%= link_to 'Show', job_catagory %></td>
        <td><%= link_to 'Edit', edit_job_catagory_path(job_catagory) %></td>
        <td><%= link_to 'Destroy', job_catagory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Catagory', new_job_catagory_path %>
